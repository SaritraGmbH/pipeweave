{
  "name": "@pipeweave/orchestrator",
  "version": "1.0.0",
  "description": "PipeWeave Orchestrator - Task execution engine",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^6.0.1",
    "@pipeweave/shared": "workspace:*",
    "chalk": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "nanoid": "^5.0.0",
    "pg-promise": "^11.5.4",
    "winston": "^3.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "eslint": "^8.55.0",
    "tsx": "^4.19.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SaritraGmbH/pipeweave.git",
    "directory": "packages/orchestrator"
  },
  "homepage": "https://github.com/SaritraGmbH/pipeweave#readme",
  "publishConfig": {
    "access": "public"
  }
}