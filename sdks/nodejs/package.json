{
  "name": "@pipeweave/sdk",
  "version": "1.0.0",
  "description": "PipeWeave SDK for Node.js - Register tasks and define pipelines",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.17.3",
    "@pipeweave/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@aws-sdk/client-s3": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-s3": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SaritraGmbH/pipeweave.git",
    "directory": "sdks/nodejs"
  },
  "homepage": "https://github.com/SaritraGmbH/pipeweave#readme",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "pipeweave",
    "workflow",
    "orchestration",
    "pipeline",
    "task",
    "sdk"
  ]
}